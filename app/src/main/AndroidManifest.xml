<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lelangapa.app">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-feature android:name="android.hardware.camera" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_new"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name="com.lelangapa.app.services.NotificationService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="com.lelangapa.app.services.NotificationInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:enabled="true" android:name="com.lelangapa.app.services.DeleteTokenService">

        </service>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.category.ItemCategoryActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.LoginActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.RegisterActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.ProfileActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.profile.EditProfileActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.profile.EditAlamatActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.profile.EditPasswordActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.profile.chat.UserChatActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.profile.chat.UserChatRoomActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.UserGeraiActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.gerai.UserLelangBarangActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.gerai.UserEditLelangBarangActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:name="com.lelangapa.app.activities.search.MainSearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.search.filter.FilterSearchActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.detail.DetailBarangActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.detail.DaftarTawaranFinalActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.favorite.FavoriteListActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.riwayat.RiwayatActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.riwayat.detailriwayat.DetailRiwayatActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.feedback.berifeedback.BeriFeedbackActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize"
            android:name="com.lelangapa.app.activities.feedback.detailfeedback.DetailBeriFeedbackActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.feedback.feedbackanda.FeedbackAndaActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.userpublic.DetailUserPublicActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.detail.block.BlockActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.lelangapa.app.activities.cropper.GalleryUtil">
        </activity>
    </application>

</manifest>